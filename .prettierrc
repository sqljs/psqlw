{
  "singleQuote": true,
  "arrowParens": "always",
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "trailingComma": "es5"
      }
    },
    {
      "files": "*.jsonc",
      "options": { "parser": "json" }
    }
  ]
}
